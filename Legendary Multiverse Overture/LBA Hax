local function DKMFYA_fake_script() -- AutoDeleteMoneyItems.AutoDeleteMoneyItems 
	local script = Instance.new('LocalScript', AutoDeleteMoneyItems)

	local DeleteMoney = false
	
	script.Parent.MouseButton1Click:Connect(function()
		DeleteMoney = not DeleteMoney
		
		while DeleteMoney do
			wait(1)
			for _,v in ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
				if v.Name == "Ruby" or v.Name == "Diamond" or v.Name == "LegendaryCoin" then
					v:Destroy()
				end
			end
		end
	end)
end
coroutine.wrap(DKMFYA_fake_script)()
local function YHLTQ_fake_script() -- AutoDeleteUselessItems.AutoDeleteUselessItems 
	local script = Instance.new('LocalScript', AutoDeleteUselessItems)

	local DeleteUseless = false
	
	script.Parent.MouseButton1Click:Connect(function()
		DeleteUseless = not DeleteUseless
		
		while DeleteUseless do
			wait(1)
			for _,v in ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
				if v.Name == "Arrow" or v.Name == "Rokakaka" or v.Name == "Bandages" or v.Name == "Water" then
					v:Destroy()
				end
			end
		end
	end)
end
coroutine.wrap(YHLTQ_fake_script)()
local function FFKR_fake_script() -- AutoPickUpItems.AutoPickUpItems 
	local script = Instance.new('LocalScript', AutoPickUpItems)

	local Pickup = false
	
	script.Parent.MouseButton1Click:Connect(function()
		Pickup = not Pickup
		
		while Pickup do
			wait(1)
			local hum = game.Players.LocalPlayer.Character.HumanoidRootPart
			table.foreach(workspace:GetChildren(), function(_, v)
				if v:IsA("Tool") then
					firetouchinterest(hum, v.Handle, 1)
					firetouchinterest(hum, v.Handle, 0)
				end
			end)
		end
	end)
end
coroutine.wrap(FFKR_fake_script)()
local function BGTQWAR_fake_script() -- AutoKillEnemies.AutoKillEnemies 
	local script = Instance.new('LocalScript', AutoKillEnemies)

	local Kill = false
	
	script.Parent.MouseButton1Click:Connect(function()
		Kill = not Kill
		
		while Kill do
			wait(1)
			for _,v in ipairs(game:GetService("Workspace"):GetChildren()) do
				if v:FindFirstChild("Humanoid") and v.Name ~= game.Players.LocalPlayer.Name and  v.Name ~= "endo_alt3" then
					local A_1 = game:GetService("Workspace")[v.Name].Humanoid
					local A_2 = CFrame.new(-11416.2676, 1866.6698, 671.755005, -0.902615368, -2.01556039e-09, -0.430447966, 1.43230752e-08, 1, -3.47168267e-08, 0.430447966, -3.75012803e-08, -0.902615368)
					local A_3 = 12000
					local A_4 = 0.1
					local A_5 = Vector3.new(21.522397994995, 1.7358413515467e-06, 45.130767822266)
					local A_6 = "rbxassetid://4863336093"
					local A_7 = 0.5
					local A_8 = Color3.new(1, 1, 1)
					local A_9 = "rbxassetid://3932141920"
					local A_10 = 1
					local A_11 = 2
					local Event = game:GetService("ReplicatedStorage").SNAKEMANDMG
					Event:FireServer(A_1, A_2, A_3, A_4, A_5, A_6, A_7, A_8, A_9, A_10, A_11)
				end
			end
		end
	end)
end
coroutine.wrap(BGTQWAR_fake_script)()
